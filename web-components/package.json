{
  "name": "notations-web",
  "version": "1.0.1",
  "description": "Web components for the Notations library - interactive notation blocks, editors, and viewers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dockview": {
      "import": "./dist/dockview.js",
      "types": "./dist/dockview.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "notations",
    "carnatic",
    "music",
    "notation",
    "web-components",
    "interactive"
  ],
  "author": "Sriram Panyam",
  "license": "ISC",
  "peerDependencies": {
    "notations": "^0.0.70",
    "dockview-core": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "dockview-core": {
      "optional": true
    }
  },
  "devDependencies": {
    "dockview-core": "^4.13.1",
    "notations": "file:../",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/panyam/notations.git",
    "directory": "web-components"
  }
}
