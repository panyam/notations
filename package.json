{
  "name": "notations",
  "version": "1.0.2",
  "author": "Sriram Panyam",
  "description": "A package for modelling, parsing, laying out carnatic musical notation",
  "homepage": "https://github.com/panyam/notations#readme",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/esm/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./web": {
      "types": "./lib/esm/web/index.d.ts",
      "import": "./lib/esm/web/index.js",
      "require": "./lib/cjs/web/index.js"
    },
    "./web/dockview": {
      "types": "./lib/esm/web/dockview.d.ts",
      "import": "./lib/esm/web/dockview.js",
      "require": "./lib/cjs/web/dockview.js"
    },
    "./styles/*": "./styles/*",
    "./dist/*": "./dist/*"
  },
  "files": [
    "lib/",
    "styles/",
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panyam/notations.git"
  },
  "keywords": [
    "notation",
    "music",
    "carnatic"
  ],
  "bugs": {
    "url": "https://github.com/panyam/notations/issues"
  },
  "dependencies": {
    "@lume/kiwi": "^0.4.4",
    "@panyam/tsutils": "*",
    "galore": "*",
    "yaml": "^2.8.2"
  },
  "peerDependencies": {
    "dockview-core": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "dockview-core": {
      "optional": true
    }
  },
  "devDependencies": {
    "dockview-core": "^4.13.1",
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-flow": "^7.25.9",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/js": "^9.39.2",
    "@types/ace": "^0.0.52",
    "@types/jest": "^29.5.14",
    "@types/request": "^2.48.12",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "autoprefixer": "^10.4.21",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "babel-preset-env": "^1.7.0",
    "browserify": "17.0.1",
    "codecov": "^3.8.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "highlight.js": "11.11.1",
    "jest": "^29.7.0",
    "jest-each": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock": "^29.7.0",
    "jsdom": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^3.5.3",
    "sass": "^1.97.1",
    "sass-loader": "^16.0.6",
    "stream-browserify": "^3.0.0",
    "style-loader": "^4.0.0",
    "css-loader": "^7.1.2",
    "ts-loader": "^9.5.0",
    "html-webpack-plugin": "^5.5.3",
    "clean-webpack-plugin": "^4.0.0",
    "ts-jest": "^29.3.1",
    "typedoc": "^0.28.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.51.0",
    "typescript-tslint-plugin": "^1.0.2",
    "webpack": "^5.99.3",
    "webpack-bundle-analyzer": "^5.1.0",
    "webpack-cli": "^6.0.1"
  },
  "pre-commit": [
    "precommit-msg",
    "clean",
    "build",
    "lintfix",
    "test"
  ],
  "scripts": {
    "clean": "rm -Rf lib && jest --clearCache && rm -rf static/lib",
    "builddocs": "npx typedoc",
    "buildbase": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "build:css": "mkdir -p dist && sass styles/NotationView.scss dist/NotationView.css --style=expanded && sass styles/NotationView.scss dist/NotationView.min.css --style=compressed",
    "build:umd": "webpack --mode=development && webpack --mode=production",
    "build:docs": "webpack --config webpack.docs.config.js --mode production",
    "build:docs:dev": "webpack --config webpack.docs.config.js --mode development",
    "build:docs:watch": "webpack --config webpack.docs.config.js --mode development --watch",
    "build": "npm run builddocs && npm run buildbase && npm run build:css && npm run build:umd",
    "test": "jest --coverage --runInBand",
    "lint": "eslint src --quiet",
    "lintfix": "eslint src --quiet --fix",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "prepare": "npm run build",
    "prepublishOnly": "echo prepublishOnly...",
    "preversion": "npm run build && npm run lintfix",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "directories": {
    "lib": "lib"
  }
}
